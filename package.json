  "homepage": "https://github.com/NikitaProkopenko/setters-test#readme",
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-plumber": "^1.2.0",
